{ "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "singleQuote": true,
  "trailingComma": "all",
  "endOfLine": "crlf",
  "importOrder": [
    "THIRD_PARTY_MODULES",
    "^@nestjs/(.*)$",
    ".*\\.module$",
    ".*\\.controller$",
    ".*\\.service$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "classProperties",
    "decorators-legacy"
  ]
}
